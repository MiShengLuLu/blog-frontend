{
  "rewrites": [
    {
      "source": "/uploads/:match*",
      "destination": "http://106.75.10.28:1337/uploads/:match*"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": ""
    },
    {
      "src": "/blog/details/(?<id>[^/]+)",
      "dest": "/blog/details?id=$id"
    },
    {
      "src": "/blog/edit/(?<id>[^/]+)",
      "dest": "/blog/edit?id=$id"
    },
    {
      "src": "/social/details/(?<id>[^/]+)/(?<api>)",
      "dest": "/social/details?id=$id&api=$api"
    }
  ]
}